{
  "functions": {
    "api/server.js": {
      "memory": 1024
    }
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" },
    { "source": "/authApi/(.*)", "destination": "/api/routes/authApi.js" },
    {
      "source": "/registarApi/(.*)",
      "destination": "/api/routes/registarApi.js"
    }
  ]
}
